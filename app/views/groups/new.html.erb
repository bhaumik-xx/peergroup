<% form_for(@group) do |f| %>
  <%#= f.error_messages %>
  <table>
    <tr>
      <td>
        <%= f.label :name %><br />
      </td>
    </tr>
    <tr>
      <td>
        <%= f.text_field :name %><br/>
      </td>
    </tr>
    <tr>
      <td>
        <%= f.label :description %><br />
      </td>
    </tr>
    <tr>
      <td>
        <%= f.text_area :description %><br/>
      </td>
    </tr>
    </table>
<table>
    <tr>
      <td width="10%">
        <%= f.submit "Submit"%>
      </td>
      <td>
        <%= link_to image_tag("button_cancel.png",:height=>"20",:width=>"90"),groups_path %>
      </td>
    </tr>
  </table>
<% end %>